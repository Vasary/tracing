{
  "name": "vasary/x-trace-id",
  "description": "Simple x-request-id trace kit",
  "keywords": [
    "trace",
    "request-id",
    "opentracing"
  ],
  "type": "library",
  "license": "0BSD",
  "authors": [
    {
      "name": "Viktor Gievoi",
      "email": "gievoi.v@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.1",
    "monolog/monolog": "^2",
    "psr/log": "^1.0.1",
    "symfony/event-dispatcher": "^5|^4|^3.4",
    "symfony/http-foundation": "^5|^4|^3.4",
    "symfony/http-kernel": "^5|^4|^3.4"
  },
  "require-dev": {
    "phpunit/php-code-coverage": "^9|^8|^7|^6",
    "phpunit/phpunit": "^9|^8|^7"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Vasary\\XTraceId\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vasary\\XTraceId\\Tests\\": "tests/"
    }
  }
}
