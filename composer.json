{
  "name": "vasary/x-trace-id",
  "description": "Simple x-request-id trace kit",
  "keywords": [
    "trace",
    "request-id",
    "opentracing"
  ],
  "type": "library",
  "license": "BSD",
  "authors": [
    {
      "name": "Viktor Gievoi",
      "email": "gievoi.v@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4",
    "monolog/monolog": "^2.0.0",
    "psr/log": "*",
    "symfony/event-dispatcher": "5.*",
    "symfony/http-foundation": "5.*",
    "symfony/http-kernel": "5.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12.64",
    "phpunit/php-code-coverage": "^9.2",
    "phpunit/phpunit": "^9"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Vasary\\XTraceId\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vasary\\XTraceId\\Tests\\": "tests/"
    }
  }
}
